{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/api/**",
        "function": "app" 
      },
      {
        "source": "**",
        "run": {
          "serviceId": "artisan-next",
          "region": "us-central1"
        }
      }
    ]
  }
}
